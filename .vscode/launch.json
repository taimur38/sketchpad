{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome",
			"type": "chrome",
			"request": "launch",
			"runtimeExecutable": "/usr/bin/google-chrome-stable",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceRoot}/src",
			"userDataDir": "${workspaceRoot}/.vscode/chrome",
			"sourceMapPathOverrides": {
			"webpack:///src/*": "${webRoot}/*"
			}
	}]
  }